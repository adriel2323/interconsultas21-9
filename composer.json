{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arcanedev/log-viewer": "^4.5",
        "barryvdh/laravel-cors": "^0.11.3",
        "barryvdh/laravel-ide-helper": "^2.4",
        "barryvdh/laravel-snappy": "^0.4.4",
        "doctrine/dbal": "~2.3",
        "dompdf/dompdf": "^0.8.2",
        "fideloper/proxy": "^4.0",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltoimage-i386": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-i386": "0.12.x",
        "infyomlabs/adminlte-templates": "5.6.x-dev",
        "infyomlabs/laravel-generator": "5.6.x-dev",
        "irazasyed/telegram-bot-sdk": "^3.0",
        "jenssegers/date": "^3.5",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6.0",
        "maddhatter/laravel-fullcalendar": "^1.3",
        "milon/barcode": "^5.3",
        "phpoffice/phpword": "^0.16.0",
        "spatie/laravel-permission": "^2.11",
        "villca/numeros-en-letras": "^1.2",
        "wildside/userstamps": "^0.5.4",
        "wilsonglasser/laravel-chatapi-whatsapp": "^1.0",
        "yajra/laravel-datatables": "^1.0",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
